{
  "name": "catastrophe",
  "version": "0.1.0",
  "homepage": "https://amatiasq.github.io/catastrophe",
  "private": true,
  "scripts": {
    "start": "parcel -o --no-hmr src/index.html",
    "build": "parcel build src/index.html"
  },
  "dependencies": {
    "amq-tools": "^3.0.2",
    "bind-decorator": "^1.0.11",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.7.0",
    "typescript": "^2.8.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-compact"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
